{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": ["airbnb-base"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "rules": {
      "class-methods-use-this": "off",
      "eqeqeq": "off",
      "no-param-reassign": "off"
    }
}  